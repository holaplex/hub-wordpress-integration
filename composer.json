{
  "name": "holaplex/holaplex-integration",
  "type": "wordpress-plugin",
  "description": "Integration plugin for Holaplex and WordPress WooCommerce",
  "authors": [
    {
      "name": "Holaplex Dev",
      "email": "dev@holaplex.com"
    }
  ],
  "require": {
    "php": ">=5.6"
  },
  "autoload": {
    "psr-4": {
      "Holaplex\\HolaplexIntegration\\": "src/"
    }
  },
  "extra": {
    "wordpress-plugin": {
      "main": "holaplex-wp.php",
      "namespace": "Holaplex\\HolaplexIntegration"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "require-dev": {
    "automattic/vipwpcs": "^2.3"
  }
}
